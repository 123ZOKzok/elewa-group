{
  "version": 2,
  "defaultProject": "elewa-group-website",
  "projects": {
    "elewa-group-website": {
      "projectType": "application",
      "root": "apps/elewa-group-website",
      "sourceRoot": "apps/elewa-group-website/src",
      "prefix": "volk",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/elewa-group-website",
            "index": "apps/elewa-group-website/src/index.html",
            "main": "apps/elewa-group-website/src/main.ts",
            "tsConfig": "apps/elewa-group-website/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/elewa-group-website/src/favicon.ico",
              "apps/elewa-group-website/src/assets",
              "apps/elewa-group-website/src/sitemap.xml"
            ],
            "styles": ["apps/elewa-group-website/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "3mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/elewa-group-website/src/environments/environment.ts",
                  "with": "apps/elewa-group-website/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "elewa-group-website:build:production"
            },
            "development": {
              "browserTarget": "elewa-group-website:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "elewa-group-website:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/elewa-group-website/src/**/*.ts",
              "apps/elewa-group-website/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/elewa-group-website"],
          "options": {
            "jestConfig": "apps/elewa-group-website/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
